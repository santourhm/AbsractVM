// Description:
//    Test if-then-else with variables
//
// Results:
//    x is positive
//    y is negative
// 
// History:
//    created on 01/01/2025

{
    int x = 5;
    int y = -3;
    
    if (x > 0) {
        println("x is positive");
    } else {
        println("x is negative");
    }
    
    if (y > 0) {
        println("y is positive");
    } else {
        println("y is negative");
    }
}
